<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Plugin.FirebaseCrashlytics</id>
    <version>4.2.0</version>
    <title>Firebase Crashlytics Plugin for Xamarin</title>
    <authors>ViewXam Devs</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/VieXam/Plugin.FirebaseCrashlytics</projectUrl>
    <description>Cross platform plugin for Firebase Crashlytics</description>
    <copyright>Copyright (c) 2021 VieXam</copyright>
    <tags>xamarin, ios, android, xamarin.forms, plugin, firebase, crashlytics</tags>
    <repository url="https://github.com/VieXam/Plugin.FirebaseCrashlytics" />
    <dependencies>
      <group targetFramework="MonoAndroid10.0">
        <dependency id="Square.OkHttp3" version="4.9.1" exclude="Build,Analyzers" />
        <dependency id="Xamarin.Firebase.Common" version="120.0.0" exclude="Analyzers" />
        <dependency id="Xamarin.Firebase.Components" version="17.0.0" exclude="Analyzers" />
        <dependency id="Xamarin.Firebase.Encoders.JSON" version="118.0.0" exclude="Analyzers" />
        <dependency id="Xamarin.Firebase.Iid" version="121.1.0" exclude="Analyzers" />
        <dependency id="Xamarin.Firebase.Iid.Interop" version="117.1.0" exclude="Analyzers" />
        <dependency id="Xamarin.Firebase.Measurement.Connector" version="119.0.0" exclude="Analyzers" />
        <dependency id="Xamarin.Google.Android.DataTransport.TransportApi" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Xamarin.Google.Android.DataTransport.TransportBackendCct" version="3.0.0" exclude="Build,Analyzers" />
        <dependency id="Xamarin.Google.Android.DataTransport.TransportRuntime" version="3.0.1" exclude="Build,Analyzers" />
        <dependency id="Xamarin.Google.Dagger" version="2.37.0" exclude="Build,Analyzers" />
        <dependency id="Xamarin.GooglePlayServices.Tasks" version="117.2.1" exclude="Analyzers" />
      </group>
      <group targetFramework="Xamarin.iOS1.0">
        <dependency id="Xamarin.Firebase.iOS.Crashlytics" version="4.6.2" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0" />
